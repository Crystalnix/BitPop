{
   "background": {
     "scripts": ["js/options.js", "js/local-cache.js", "js/desktop_notifications.js", "js/background.js"]
   },
   "browser_action": {
      "default_icon": "images/icon19.png",
      "default_popup": "popup.html",
      "default_title": "Facebook Messages"
   },
   "description": "Keep up with your friends, even when you're not on Facebook.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
   },
   "name": "Facebook Notify Messages",
   "permissions": [ "notifications", "tabs", "chrome://favicon/" ],
   "version": "1.13",
   "update_url": "http://tools.bitpop.com/ext/updates.xml",
   "manifest_version": 2
}
