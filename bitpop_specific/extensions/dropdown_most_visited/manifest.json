{
   "background": {
     "scripts": ["js/background.js"]
   },
   "description": "Displays a list of sites you visited the most in a dropdown list",
   "icons": {
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
   },
   "name": "Dropdown List of Most Visited Links",
   "options_page": "options.html",
   "page_action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html",
      "default_title": "Shows most visited sites."
   },
   "permissions": [ "tabs", "history", "bookmarks", "chrome://favicon/" ],
   "version": "0.15",
   "update_url": "http://tools.bitpop.com/ext/updates.xml",
   "manifest_version": 2
}
