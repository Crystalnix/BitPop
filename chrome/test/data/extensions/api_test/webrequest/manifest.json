{
  "name": "webRequest",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tests the webRequest API.",
  "permissions": ["webRequest", "webRequestBlocking", "tabs",
    "declarativeWebRequest", "http://*.a.com/*", "https://*.a.com/*",
    "http://non_existing_third_party.com/image.png"],
  "web_accessible_resources": [
    "simpleLoad/a.html",
    "complexLoad/a.html",
    "complexLoad/b.html",
    "does_not_exist.html",
    "requestBody/multipart.html",
    "requestBody/no-enctype.html",
    "requestBody/plaintext.html",
    "requestBody/urlencoded.html"
  ]
}
